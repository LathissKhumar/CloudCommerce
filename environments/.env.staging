# Staging Environment Configuration
NODE_ENV=staging
PORT=5000
MONGO_URI=mongodb://cloudcommerce-mongo:27017/cloudcommerce_staging
JWT_SECRET=staging_jwt_secret_key_change_me
CORS_ORIGIN=https://staging.cloudcommerce.example.com

# Admin Credentials
ADMIN_EMAIL=admin@staging.cloudcommerce.com
ADMIN_PASSWORD=staging_admin_password

# Features
ENABLE_LOGGING=true
ENABLE_DEBUG=false
ENABLE_SEED_DATA=false

# Registry
DOCKER_REGISTRY=staging-registry.example.com
